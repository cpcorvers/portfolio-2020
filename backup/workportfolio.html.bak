---
layout: default
title: Workportfolio
permalink: /work/
---
<!-- {% assign blogs = site.posts | where: "category", "work" %} -->

<div class="portfolio-container">

  {% for post in site.posts %}
  <div class="portfolio-cells">
    <div class="portfolio-cellA" style="background-image:url({{ site.pathtoimages }}{{ post.thumbnail }})">

      <a href="{{ post.url }}" class="portfolio">

        <div class="portfolio-link">

          <div class="portfolio-link-text">
            {{ post.url }}
          </div>

        </div>

      </a>

    </div>

    <div class="portfolio-cellB">
      <p> {{ post.title }} </p>
    </div>
  </div>
  {% endfor %}

</div>
